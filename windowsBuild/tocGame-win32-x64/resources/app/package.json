{
    "name": "toc-game",
    "version": "0.0.1",
    "description": "Versi√≥n v2 del tocGame",
    "author": {
        "name": "Ezequiel"
    },
    "main": "build/main.js",
    "scripts": {
        "compile": "tsc && copyfiles -f web/index.html build/web",
        "start": "npm run compile && electron build/main.js",
        "build:windows": "npm run compile && electron-packager . tocGame --out=windowsBuild",
        "build:linux": "npm run compile && electron-packager . tocGame --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=linuxBuild",
        "reparar": "npx electron-rebuild -f -p"
    },
    "dependencies": {},
    "devDependencies": {
        "copyfiles": "2.1.1",
        "electron": "6.0.0",
        "electron-packager": "^14.2.1",
        "electron-rebuild": "^1.10.1",
        "typescript": "3.5.3"
    }
}
